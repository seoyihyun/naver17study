package day1216;

public class ArrayPractice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 배열(Array) 타입 ; 연속된 공간에 값을 나열시키고 각 값에 인덱스를 부여한 구조
		// 같은 타입의 값만 관리하며 배열의 길이는 증감할 수 없음
		
		// 배열 변수 선언
		int[]intArray;
		String[]StringArray;
		
		int intArray1[];
		String StringArray1[];
		
		//1차원 배열 생성하기
		
		// 값 목록으로 배열 생성
		int[]intArray3 = {0,1,2,3};
		String[]seasons = {Spring, Summer, Fall, Winter};
		// 생성된 배열에 목록을 대입하는 경우
		int = new int[] {4,5,6,7};
		
		// new 연산자로 배열 생성: 해당 인덱스만큼의 배열 생성
		String[] str1 = new String[3];
		// 생성된 배열에 값을 대입하는 경우
		int[2] = 10;
		
		// 다차원 배열 생성하기
		
		// 값 목록으로 배열 생성하기
		String kim, lee, seo, kang;
		String [][] names = {
				{kim, lee, seo, kang, lee}
				{seo, kang}
		};
		
		
	}

}
