package day1220;

public class Ex2Inheri {

	class SuperObj2 {
		private String schoolName; // 자식클래스도 접근할 수 없음

		public SuperObj2() {
			schoolName = "정담고";
		}
		SuperObj2(String schoolName) {
			this.schoolName = schoolName;
		}
		
		public void write() {
			System.out.println("학교명 : "+schoolName);
		}
	}

	class SubObh2 {

	}

