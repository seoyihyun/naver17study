package day1211;

public class Ex4DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int year=2024;
		int month=12;
		int day=11;
		
		int year2=2024;
		int month2=5;
		int day2=3;
		
		//2024-12-11 형태로 출력
		System.out.println(year+"-"+month+"-"+day);
	}

}
