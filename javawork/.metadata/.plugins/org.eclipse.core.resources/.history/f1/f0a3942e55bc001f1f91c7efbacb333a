package day1217;

public class ArrayPractice1217 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] data = {1,2,3,4,5};
		int max = data[0];
		int min = data[0];
		
		for(int i=1; i<data.length; i++)
		{
			if(max<data[i])
			{
				max = data[i];
			}
			if(min>data[i])
			{
				min = data[i];
			}
		}
		System.out.println(max);
		System.out.println(min);

	}

}
