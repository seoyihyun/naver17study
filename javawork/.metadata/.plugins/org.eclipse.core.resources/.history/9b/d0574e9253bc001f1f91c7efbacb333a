package day1217;

public class ArrayPractice1217 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		char [] ch;
		ch = new char[3];
		ch[0] = 'H';
		ch[1] = 'A';
		ch[2] = 'a';
		
		for(int i=0; i<ch.length; i++)
			System.out.println("ch[i] = "+ch[i]);
		for(char a : ch)
			System.out.println(a);
		
		char [] ch = new char[] {'A', 'B', 'C'};
		for(int i=0; i<ch.length; i++)
			System.out.println("ch[i] = "+ch[i]);

	}

}
