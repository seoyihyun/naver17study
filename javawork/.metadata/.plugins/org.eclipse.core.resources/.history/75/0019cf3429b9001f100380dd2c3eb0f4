package day1213;

public class Ex11For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		System.out.println("알파벳 반복해서 출력하기");
		for(int i=65; i<=90; i++)
			System.out.print((char)i+ " "); // 숫자를 캐릭터형으로 변환하여 출력(char)
		System.out.println();
		
		for(char ch='A'; ch<=90; ch++)
			System.out.print(ch+ " ");
		System.out.println();
		System.out.println();
		
		System.out.println("알파벳 거꿀로 출력하기");
		
		for(int i=90; i>=65; i--)
			System.out.print((char)i + " ");
		System.out.println();
		
		for(char i=90; i>=65; i--)
			System.out.print(i+" ");
		System.out.println();
				

	}

}
