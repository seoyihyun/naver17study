public class Ex10Gugudan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i=2; i<=9; i++)
		{
			System.out.print("["+i+"ë‹¨]   ");
		}
		System.out.println();
		System.out.println("=".repeat(61));
		
		for (int i=2; i<=9; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j"="+(i*j));
				
			}
		}

	}

}
