package day1220;

class SuperObj1 {
	
	// 생성자 선언
	SuperObj1() {
		System.out.println("super 생성자");
	}
}

class SubObj1 extends SuperObj1 {
	SubObj1() {
		System.out.println("sub 생성자");
	}
}

