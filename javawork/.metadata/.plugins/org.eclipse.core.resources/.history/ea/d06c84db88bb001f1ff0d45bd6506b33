package day1216;

public class Ex10Gugudan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i=2; i<=9; i++)
		{
			System.out.print("["+i+"ë‹¨]     ");
		}
		System.out.println();
		System.out.println("=".repeat(75));
		
		for (int i=2; i<=9; i++)
		{
			for(int j=1; j<=1; j++)
			{
				System.out.print(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
		
		for (int i=2; i<=9; i++)
		{
			for(int j=1; j<=2; j++)
			{
				System.out.print(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
		
	}

}
