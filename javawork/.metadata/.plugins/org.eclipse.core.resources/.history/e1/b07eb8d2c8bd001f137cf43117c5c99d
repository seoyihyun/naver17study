package day1219;

public class Ex7VarArgs {

	public static void main(String[] args) {

		public static int calcNum(int ... n ) {
			int sum = 0;
			System.out.println("총"+n+"개의 점수");
			for (int score : n) {
				System.out.println(score+" ");
				sum+=score;
			}
			System.out.println();
			return sum;
		}
		// TODO Auto-generated method stub

	}

}
