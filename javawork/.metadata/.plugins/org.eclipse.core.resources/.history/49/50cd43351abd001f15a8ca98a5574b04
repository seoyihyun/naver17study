package day1218;

class Drink
{	
	// 필드 선언
	String model = "우유";
	String color;
	int bottle;
	
	// 생성자 선언
	public static void Drink(String c, int b)
	{
		color = c;
		bottle = b;
	}
}

public class ObjectNewPractice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 생성자 선언
		Drink milk = new Drink(null, 0);
		
		

	}

}
