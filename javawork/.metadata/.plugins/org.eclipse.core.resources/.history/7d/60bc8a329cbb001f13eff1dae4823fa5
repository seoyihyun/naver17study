package day1216;

public class ArrayPractice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 배열(Array) 타입 ; 연속된 공간에 값을 나열시키고 각 값에 인덱스를 부여한 구조
		// 같은 타입의 값만 관리하며 배열의 길이는 증감할 수 없음
		
		// 배열 변수 선언
		int[]intArray;
		String[]StringArray;
		
		int intArray1[];
		String StringArray1[];
		
		// 값 목록으로 배열 선언
		int[]intArray3 = {0,1,2,3};
		String[]seasons = {Spring, Summer, Fall, Winter};
		
		
	}

}
