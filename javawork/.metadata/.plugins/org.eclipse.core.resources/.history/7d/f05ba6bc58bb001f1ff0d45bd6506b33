package day1216;

public class Ex4ForLable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Loop : // 레이블 명은 맘대로~~
			for(int i=1; i<=3; i++)
			{
				for(int j=1; j<=4; j++)
				{
//					if(i==2)
//						continue Loop; // for문의 조건식인 i++로 이동, i==2 케이스를 출력하지 않음
					if(j==2)
						continue Loop;
					System.out.println("i="+i+", j="+j);
				}
			}

	}

}
