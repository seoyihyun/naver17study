		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
		
		for (int i=3; i<=3; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
		
		for (int i=4; i<=4; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
		
		for (int i=5; i<=5; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
		
		for (int i=6; i<=6; i++)
		{
			for(int j=1; j<=9; j++)
