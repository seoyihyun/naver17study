			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}

		for (int i=9; i<=9; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i+"x"+j+"="+(i*j)+"     ");
				
			}
		}
